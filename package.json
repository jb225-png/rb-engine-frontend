{
  "name": "rbb-engine-ela-frontend",
  "version": "2.0.0",
  "description": "ELA Template Generation System - Christian-facing educational content",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^5.17.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-scripts": "5.0.1",
